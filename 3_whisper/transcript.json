    {
      "id": 2,
      "start": 28.0,
      "end": 30.0,
      "text": "يا عائشة لماذا تختبئين هنا؟"
    },
    {
      "id": 3,
      "start": 30.260000228881836,
      "end": 33.2599983215332,
      "text": "أليس هذا العنب من علبة وجبتك المدرسية؟"
    },
    {
      "id": 4,
      "start": 33.5,
      "end": 34.400001525878906,
      "text": "فلماذا تأكلينها الآن؟"
    },
    {
      "id": 5,
      "start": 35.58000183105469,
      "end": 37.0,
      "text": ""
    },
    {
      "id": 6,
      "start": 37.08000183105469,
      "end": 40.0,
      "text": "لأن هذه الحركات قد تصيبك بجروح خطيرة"
    },
    {
      "id": 7,
      "start": 41.279998779296875,
      "end": 41.560001373291016,
      "text": "أخي عمار"
    },
    {
      "id": 8,
      "start": 42.81999969482422,
      "end": 45.0,
      "text": "هذا العنب لذيذ جدا"
    },
    {
      "id": 9,
      "start": 45.18000030517578,
      "end": 46.0,
      "text": "تفضل"
    },
    {
      "id": 10,
      "start": 46.099998474121094,
      "end": 47.0,
      "text": ""
    },
    {
      "id": 11,
      "start": 47.0,
      "end": 48.08000183105469,
      "text": "بارك الله فيك يا عائشة"
    },
    {
      "id": 12,
      "start": 49.0,
      "end": 52.0,
      "text": "هي بن لنغسل اليدين والفم قبل الفطور"
    },
    {
      "id": 13,
      "start": 55.0,
      "end": 58.0,
      "text": "سأعلمك اليوم كيفية غسيل يدين والفم"
    },
    {
      "id": 14,
      "start": 58.79999923706055,
      "end": 59.400001525878906,
      "text": "بالطريقة الصحية"
    },
    {
      "id": 15,
      "start": 59.400001525878906,
      "end": 60.599998474121094,
      "text": "هل نغسل يدين والفم؟"
    },
    {
      "id": 16,
      "start": 60.599998474121094,
      "end": 61.599998474121094,
      "text": "نعم يا أختي"
    },
    {
      "id": 17,
      "start": 61.599998474121094,
      "end": 63.599998474121094,
      "text": "أولا نبدأ بالتسمية"
    },
    {
      "id": 18,
      "start": 63.599998474121094,
      "end": 65.5999984741211,
      "text": "أين نقول بسم الله الرحمن الرحيم"
    },
    {
      "id": 19,
      "start": 65.5999984741211,
      "end": 68.5999984741211,
      "text": "ثم نفتح صنبور الماء باليد اليمنى"
    },
    {
      "id": 20,
      "start": 68.5999984741211,
      "end": 70.5999984741211,
      "text": "وبعد ذلك نبلل اليدين إلى الرسغ"
    },
    {
      "id": 21,
      "start": 70.5999984741211,
      "end": 73.5999984741211,
      "text": "ثم نأخذ الصابون ونبلله"
    },
    {
      "id": 22,
      "start": 73.5999984741211,
      "end": 75.5999984741211,
      "text": "وبعدها نغلق صنبور الماء"
    },
    {
      "id": 23,
      "start": 75.5999984741211,
      "end": 78.5999984741211,
      "text": "ثم نفرق اليدين بالصابون جيدا"
    },
    {
      "id": 24,
      "start": 79.5999984741211,
      "end": 81.5999984741211,
      "text": "بعد ذلك نفتح صنبور الماء"
    },
    {
      "id": 25,
      "start": 81.5999984741211,
      "end": 84.5999984741211,
      "text": "ونغسل اليدين حتى تتم إزالة الصابون"
    },
    {
      "id": 26,
      "start": 86.5999984741211,
      "end": 88.5999984741211,
      "text": "ثم نمضمض الفم بشكل جيد"
    },
    {
      "id": 27,
      "start": 89.5999984741211,
      "end": 91.5999984741211,
      "text": "وكذلك نأخذ الماء باليد"
    },
    {
      "id": 28,
      "start": 91.5999984741211,
      "end": 94.5999984741211,
      "text": "وننظف مقدم الفم بعناية جيدة"
    },
    {
      "id": 29,
      "start": 94.5999984741211,
      "end": 96.5999984741211,
      "text": "ثم نغلق صنبور الماء فورا"
    },
    {
      "id": 30,
      "start": 98.5999984741211,
      "end": 99.5999984741211,
      "text": "أحبّة الصغار"
    },
    {
      "id": 31,
      "start": 99.5999984741211,
      "end": 102.5999984741211,
      "text": "عليكم أن تقوموا بالوضوء للطعام قبل تناوله"
    },
    {
      "id": 32,
      "start": 102.5999984741211,
      "end": 105.5999984741211,
      "text": "أي اغسلوا اليدين والفم جيدا"
    },
    {
      "id": 33,
      "start": 105.5999984741211,
      "end": 106.5999984741211,
      "text": "ففي ذلك بركة"
    },
    {
      "id": 34,
      "start": 106.5999984741211,
      "end": 109.5999984741211,
      "text": "واتباع لسنة النبي صلى الله عليه وسلم"
    },
    {
      "id": 35,
      "start": 109.5999984741211,
      "end": 111.5999984741211,
      "text": "ولا تسرفوا الماء بلا فائدة"
    },
    {
      "id": 36,
      "start": 111.5999984741211,
      "end": 112.5999984741211,
      "text": ""
    },
    {
      "id": 37,
      "start": 112.5999984741211,
      "end": 114.5999984741211,
      "text": "اشتركوا في القناة"
    },
